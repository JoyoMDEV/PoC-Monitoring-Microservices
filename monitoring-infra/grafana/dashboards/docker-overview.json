{
    "__requires": [
        {
            "type": "panel",
            "id": "singlestat",
            "name": "Singlestat",
            "version": ""
        },
        {
            "type": "panel",
            "id": "graph",
            "name": "Graph",
            "version": ""
        },
        {
            "type": "grafana",
            "id": "grafana",
            "name": "Grafana",
            "version": "3.1.0"
        },
        {
            "type": "datasource",
            "id": "prometheus",
            "name": "Prometheus",
            "version": "1.0.0"
        }
    ],
    "id": null,
    "title": "Docker System Monitoring (Kompakt)",
    "description": "Kompaktes Docker Monitoring Dashboard, kompatibel zu Ã¤lteren Grafana Versionen. cAdvisor/Prometheus.",
    "tags": [
        "docker"
    ],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "sharedCrosshair": true,
    "rows": [
        {
            "collapse": false,
            "editable": true,
            "height": "50",
            "panels": [
                {
                    "type": "singlestat",
                    "title": "Running containers",
                    "id": 1,
                    "span": 4,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "count(container_last_seen{image!=\"\"})",
                            "refId": "A"
                        }
                    ],
                    "format": "none",
                    "valueName": "current",
                    "valueFontSize": "80%",
                    "colors": [
                        "rgba(245, 54, 54, 0.9)",
                        "rgba(237, 129, 40, 0.89)",
                        "rgba(50, 172, 45, 0.97)"
                    ],
                    "colorBackground": false,
                    "transparent": true
                },
                {
                    "type": "singlestat",
                    "title": "Total Memory Usage",
                    "id": 2,
                    "span": 4,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "sum(container_memory_usage_bytes{image!=\"\"})/1024/1024",
                            "refId": "A"
                        }
                    ],
                    "format": "mbytes",
                    "valueName": "current",
                    "valueFontSize": "80%",
                    "colors": [
                        "rgba(245, 54, 54, 0.9)",
                        "rgba(237, 129, 40, 0.89)",
                        "rgba(50, 172, 45, 0.97)"
                    ],
                    "colorBackground": false,
                    "transparent": true
                },
                {
                    "type": "singlestat",
                    "title": "Total CPU Usage",
                    "id": 3,
                    "span": 4,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "sum(rate(container_cpu_user_seconds_total{image!=\"\"}[5m]) * 100)",
                            "refId": "A"
                        }
                    ],
                    "format": "percent",
                    "valueName": "current",
                    "valueFontSize": "80%",
                    "colors": [
                        "rgba(245, 54, 54, 0.9)",
                        "rgba(237, 129, 40, 0.89)",
                        "rgba(50, 172, 45, 0.97)"
                    ],
                    "colorBackground": false,
                    "transparent": true
                }
            ],
            "title": "Summary"
        },
        {
            "collapse": false,
            "editable": true,
            "height": "250px",
            "panels": [
                {
                    "type": "graph",
                    "title": "CPU Usage per Container",
                    "id": 4,
                    "span": 12,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "rate(container_cpu_user_seconds_total{image!=\"\"}[5m]) * 100",
                            "refId": "A",
                            "legendFormat": "{{name}}"
                        }
                    ],
                    "yaxes": [
                        {
                            "format": "percent",
                            "show": true
                        },
                        {
                            "format": "short",
                            "show": true
                        }
                    ],
                    "lines": true,
                    "linewidth": 2,
                    "nullPointMode": "connected",
                    "legend": {
                        "alignAsTable": true,
                        "avg": true,
                        "current": true,
                        "show": true,
                        "values": true
                    }
                }
            ],
            "title": "CPU"
        },
        {
            "collapse": false,
            "editable": true,
            "height": "250px",
            "panels": [
                {
                    "type": "graph",
                    "title": "Memory Usage per Container",
                    "id": 5,
                    "span": 12,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "container_memory_usage_bytes{image!=\"\"}",
                            "refId": "A",
                            "legendFormat": "{{name}}"
                        }
                    ],
                    "yaxes": [
                        {
                            "format": "bytes",
                            "show": true
                        },
                        {
                            "format": "short",
                            "show": false
                        }
                    ],
                    "lines": true,
                    "linewidth": 2,
                    "nullPointMode": "connected",
                    "legend": {
                        "alignAsTable": true,
                        "avg": true,
                        "current": true,
                        "show": true,
                        "values": true
                    }
                }
            ],
            "title": "Memory"
        },
        {
            "collapse": false,
            "editable": true,
            "height": "250px",
            "panels": [
                {
                    "type": "graph",
                    "title": "Network Rx",
                    "id": 6,
                    "span": 6,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "irate(container_network_receive_bytes_total{image!=\"\"}[5m])",
                            "refId": "A",
                            "legendFormat": "{{name}}"
                        }
                    ],
                    "yaxes": [
                        {
                            "format": "Bps",
                            "show": true
                        },
                        {
                            "format": "short",
                            "show": true
                        }
                    ],
                    "lines": true,
                    "linewidth": 2,
                    "nullPointMode": "connected",
                    "legend": {
                        "show": true
                    }
                },
                {
                    "type": "graph",
                    "title": "Network Tx",
                    "id": 7,
                    "span": 6,
                    "datasource": "prometheus",
                    "targets": [
                        {
                            "expr": "irate(container_network_transmit_bytes_total{image!=\"\"}[5m])",
                            "refId": "A",
                            "legendFormat": "{{name}}"
                        }
                    ],
                    "yaxes": [
                        {
                            "format": "Bps",
                            "show": true
                        },
                        {
                            "format": "short",
                            "show": true
                        }
                    ],
                    "lines": true,
                    "linewidth": 2,
                    "nullPointMode": "connected",
                    "legend": {
                        "show": true
                    }
                }
            ],
            "title": "Network"
        }
    ],
    "time": {
        "from": "now-3h",
        "to": "now"
    },
    "timepicker": {
        "refresh_intervals": [
            "5s",
            "10s",
            "30s",
            "1m",
            "5m",
            "15m",
            "30m",
            "1h",
            "2h",
            "1d"
        ],
        "time_options": [
            "5m",
            "15m",
            "1h",
            "6h",
            "12h",
            "24h",
            "2d",
            "7d",
            "30d"
        ]
    },
    "templating": {
        "list": []
    },
    "annotations": {
        "list": []
    },
    "refresh": "10s",
    "schemaVersion": 12,
    "version": 1,
    "links": [],
    "gnetId": null
}